  "bugs": {
    "url": "https://github.com/squteronsquter/simple-tailwind-starter/issues"
  },
  "homepage": "https://github.com/squteronsquter/simple-tailwind-starter#readme",
  "devDependencies": {
    "tailwindcss": "^3.2.4"
  }
}
